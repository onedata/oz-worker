<div class="accordion tokens-accordion">
  {{#each tokens as |t|}}
    {{tokens-list-item token=t}}
  {{/each}}

  <div class="accordion-group tokens-accordion-group">
    <div class="accordion-heading tokens-accordion-heading">
      <a class="accordion-toggle tokens-accordion-toggle create-new"
        {{action 'createNewToken'}}>
        <div class="token-icon">
          {{one-icon icon='add'}}
        </div>
        <div class="token-header">
          {{t 'onezone.tokensAccordion.createNewToken'}}
        </div>
      </a>
    </div>
  </div>

</div>
